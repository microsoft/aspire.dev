{
  "servers": {
    "aspire": {
      "type": "stdio",
      "command": "aspire",
      "args": [
        "mcp",
        "start"
      ]
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest"
      ]
    },
    "hex1b": {
      "type": "stdio",
      "command": "dnx",
      "args": [
        "Hex1b.McpServer@0.66.0",
        "--yes"
      ]
    }
  }
}